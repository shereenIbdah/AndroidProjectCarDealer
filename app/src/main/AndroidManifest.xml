<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AndroidProject"
        tools:targetApi="31" >
        <activity
            android:name=".HomeAdmin"
            android:exported="false"
            android:label="@string/title_activity_home_admin"
            android:theme="@style/Theme.AndroidProject.NoActionBar" />
        <activity
            android:name=".SignUpAsAdmin"
            android:exported="false" />
        <activity
            android:name=".SignInAsAdmin"
            android:exported="false" />
        <activity
            android:name=".SignInChoices"
            android:exported="false" />
        <activity
            android:name=".Home"
            android:exported="false"
            android:label="@string/title_activity_home"
            android:theme="@style/Theme.AndroidProject.NoActionBar" />
        <activity
            android:name=".SignInActivity"
            android:exported="false" />
        <activity
            android:name=".SingUpActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>